# vim: ft=python

j6 = module("j6",
    kind = "lib",
    includes = [ "include" ],
    sources = [
        "init.cpp",
    ])

from glob import glob
definitions = glob('definitions/**/*.def', recursive=True)
j6.add_input("include/j6/syscalls.h.cog", deps=definitions)
j6.add_input("syscalls.s.cog", deps=definitions)

