object channel : kobject {
    uid 3ea38b96aa0e54c8

    capabilities [
        send
        receive
        close
    ]

    method create [constructor] 
    method close [destructor cap:close]

    method send [cap:send] {
        param data buffer [inout]
    }

    method receive [cap:receive] {
        param data buffer [out]
    }
}
